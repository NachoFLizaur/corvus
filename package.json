{
  "name": "corvus-ai",
  "version": "0.1.1",
  "description": "Corvus AI agent suite for OpenCode â€” multi-agent orchestration with specialized agents, commands, and skills",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "corvus-ai": "bin/cli.mjs"
  },
  "exports": {
    ".": {
